all: $(PROJECTS)

chipmunk:
ifneq (,$(chipmunk_config))
	@echo "==== Building chipmunk ($(chipmunk_config)) ===="
	@${MAKE} --no-print-directory -C thirdparty -f chipmunk.make config=$(chipmunk_config)
endif

